  <winds>
    <wind strname="dong" msg="东风" val="270"/>
    <wind strname="dongnan" msg="东南风" val="315"/>
    <wind strname="nan" msg="南风" val="360"/>
    <wind strname="xinan" msg="西南风" val="45"/>
    <wind strname="xi" msg="西风" val="90"/>
    <wind strname="xibei" msg="西北风" val="135"/>
    <wind strname="bei" msg="北风" val="180"/>
    <wind strname="dongbei" msg="东北风" val="225"/>
  </winds>
  <buttons>
    <button parametername="wind" eventclass="com.vanwins.chart.panel.Column" text="风向风速" bgcolor="0x00ffff" sbgcolor="0x00ff00" sfontcolor="0x00ff00" fontcolor="0x004D99" defaultselect="1"/>
    <button parametername="rainfall" eventclass="com.vanwins.chart.panel.Column" text="降雨量" bgcolor="0x00ff00" sbgcolor="0x00ff00" sfontcolor="0x00ff00" fontcolor="0x004D99" defaultselect="0"/>
    <button parametername="temperature" eventclass="com.vanwins.chart.panel.Dotted" text="气温" bgcolor="0x00ffff" sbgcolor="0x00ff00" sfontcolor="0x00ff00" fontcolor="0x004D99" defaultselect="0"/>
    <button parametername="relativehumidity" eventclass="com.vanwins.chart.panel.Dotted" text="相对湿度" bgcolor="0x00ffff" sbgcolor="0x00ff00" sfontcolor="0x00ff00" fontcolor="0x004D99" defaultselect="0"/>
